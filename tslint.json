{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark" : [true, "single"],
        "ordered-imports": false,
        "trailing-comma": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "only-arrow-functions": false,
        "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
}
